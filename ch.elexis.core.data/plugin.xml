            categoryId="ch.elexis.commands.bills"
            defaultHandler="ch.elexis.commands.ErstelleRnnCommand"
            id="bill.create"
            name="%command.name.1">
         <commandParameter
               id="ch.elexis.RechnungErstellen.parameter"
               name="%commandParameter.name.0"
               optional="true"
               typeId="ch.elexis.TreeType">
         </commandParameter>
      </command>
      <category
