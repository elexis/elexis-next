                  commandId="org.eclipse.ui.views.showView"
                  icon="rsc/new_wiz.gif"
                  label="Views anzeigen"
                  style="push"
                  tooltip="%command.showViews">
               <visibleWhen
                     checkEnabled="false">
                  <test
                        args="AC_SHOWVIEW"
                        property="ch.elexis.commands.propertyTester.ACE">
                  </test>
               </visibleWhen>
            </command>
         </toolbar>
         <toolbar
               id="ch.elexis.coolbar.MandantSelection">
            <dynamic
                  class="ch.elexis.coolbar.MandantSelectionContributionItem"
                  id="ch.elexis.coolbar.MandantSelectionContributionItem">
            </dynamic>
         </toolbar>
      </menuContribution>
   </extension>
