            description="%command.description.2"
            id="bill.reminder"
            name="%command.name.2">
      </command>
      <commandParameterType
            converter="ch.elexis.commands.TreeToStringConverter"
            id="ch.elexis.TreeType"
            type="ch.elexis.util.Tree">
      </commandParameterType>
      <command
            categoryId="ch.elexis.commands.kategorie"
            defaultHandler="ch.elexis.commands.ExportiereBloeckeCommand"
            description="%command.description.3"
            id="serviceblocks.export"
            name="%command.name.3">
